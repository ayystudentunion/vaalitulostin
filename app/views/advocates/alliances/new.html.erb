<p>
  &larr; <%= link_to "Palaa tallentamatta".html_safe, advocates_alliances_path %>
</p>


<%= semantic_form_for @alliance, :url => advocates_alliances_path(@alliance) do |f| %>

  <%= render :partial => "form", :locals => { :alliance => @alliance, :f => f } %>

  <% f.actions do %>
    <%= f.action :submit, :as => :button, :label => "Tallenna" %>
  <% end %>
<% end %>