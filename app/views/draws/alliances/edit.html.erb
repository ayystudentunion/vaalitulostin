<h1>Liittojen väliset arvonnat</h1>

<p><%= link_to 'Takaisin liittojen välisiin arvontoihin', draws_alliances_path %></p>

<% form_tag do -%>
<table>
<% @alliance_draw.alliance_drawings.each do |drawing| -%>
  <tr>
    <td><%= drawing.electoral_alliance.shorten %></td>
    <td><input type="text" /></td>
  </tr>
<% end -%>
</table>

<input type="submit" value="Tallenna" />
<% end -%>
