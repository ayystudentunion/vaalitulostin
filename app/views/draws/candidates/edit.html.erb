<h1>Liittojen sisäiset arvonnat</h1>

<p><%= link_to 'Takaisin liittojen välisiin arvontoihin', draws_candidates_path %></p>

<% form_tag do -%>
<table>
  <% @candidate_draw.candidate_drawings.each do |drawing| -%>
  <tr>
    <td><%= drawing.candidate.candidate_name %></td>
    <td><input type="text" /></td>
  </tr>
<% end -%>
</table>

<input type="submit" value="Tallenna" />
<% end -%>
