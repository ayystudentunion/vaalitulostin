<h2>Luo uusi äänestäjä</h2>

<%= form_for @api_voter, url: manage_voters_path do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %>
  <br>

  <%= f.label :email %><br>
  <%= f.text_field :email %>
  <br>

  <%= f.label :ssn %><br>
  <%= f.text_field :ssn %>
  <br>

  <%= f.label :student_number %><br>
  <%= f.text_field :student_number %>
  <br>

  <%= f.label :phone %><br>
  <%= f.text_field :phone %>
  <br>

  <%= f.label :faculty_code %><br>
  <%= collection_select(:faculty, :faculty_code, Faculty.all, :code, :name) %>
  <br>

  <%#= f.label :department_code %><br>
  <%#= collection_select(:department, :department_code, Department.all, :code, :name) %>

  <%= f.submit "Luo äänestäjä", class: "btn btn-primary" %>
<% end %>
