<%= raw(JSONBuilder::Compiler.generate do
  name "Äänestysalueen mukaan"
  year Time.now.year
  created_at Time.now.localtime

  children do
    array Voter.count_by_area do |area_id, vote_sum|
      better_area = VotingArea.find(area_id) # urgh.. sorry for shortcuts.

      name   better_area.name
      code   better_area.code
      votes  vote_sum
    end
  end

end) %>
